  <style>
    .sr-only{
      clip-path:none;
      left:15px;
      right:44px;
      text-align: left;
      top:35px;
      bottom: 16px;
      white-space:normal;
    }
    .navbar-nav>li>a{
      text-align: left;

    }
    ul, menu, dir {
    display: block;
    list-style-type: disc;
    -webkit-margin-before: 1em;
    -webkit-margin-after: 1em;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
    -webkit-padding-start: 40px;
    }
    .nav>li{
      -webkit-min-logical-height:0px;
      -webkit-min-logical-width:0px;
      -webkit-locale:"en";
      text-align:left;
      position: static;
      min-width:0px;
      min-inline-size:0px;
      min-height:0px;
      min-block-size:0px;

      float:left;
          }
    .navbar{
      align-items: normal;
      display:block;
      flex-wrap:nowrap;
      font-weight:400;
    }
    .navbar-brand{
      margin-right: 0px;
    }
    .container-fluid>.navbar-collapse, 
    .container-fluid>.navbar-header, 
    .container>.navbar-collapse, 
    .container>.navbar-header{
      align-items:normal;
      -webkit-locale:"en";
      flex-basis: auto;
      flex-grow:0;
    }
    .navbar-default{
      margin : 0px;
      padding : 0px;
      position: relative;
      min-height: 50px;
      margin-bottom: 20px;
      border: 1px solid transparent;
      border-bottom-color: rgba(0, 0, 0, 0);
      align-items: normal;
      display:block;
      flex-wrap:nowrap;
      justify-content: normal;
    }
    .navbar > .container-fluid{
      align-items: normal;
      display:block;
      flex-wrap:nowrap;
      font-weight:400;
      justify-content: normal;
      min-block-size:0px;
      min-height:0px;
      min-inline-size:0px;
      min-width: 0px;
      text-align:start;
      -webkit-locale:auto;
      -webkit-min-logical-height:0px;
      -webkit-min-logical-width:0px;

    }
    .navbar-nav{
      display: block;
    }
    .nav{
      list-style-type: : disc;
    }
    .dropdown-toggle::after{
      display: none;
    }
  </style>
